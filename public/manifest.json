{
  "manifest_version": 3,
  "name": "My vite Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "Icon.png",
    "48": "Icon.png",
    "128": "icon128.png"
  }
}
